{
  "compilerOptions": {
    "lib": [
      "dom",
      "ES2021",
      "DOM.Iterable"
    ],
	"target": "es2024",
    "module": "esnext",
    "noEmitOnError": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
	"noImplicitAny": false,
    "strict": true,
    "skipLibCheck": true
  }
}
