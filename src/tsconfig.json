{
  "compilerOptions": {
    "target": "ES5",
    "lib": [
      "dom",
      "ES2021",
      "DOM.Iterable"
    ],
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "baseUrl": "src",
    "noEmitOnError": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
	"noImplicitAny": false,
    "strict": true,
    "skipLibCheck": true,
  },
}
